<?xml version="1.0" encoding="US-ASCII"?>
<xml-bindings xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm"
              package-name="com.gordondickens.jerseyrest.domain">
    <java-types>
        <java-type name="Item">
            <xml-root-element/>
            <xml-type name="item" prop-order="name description" />
            <java-attributes>
                <xml-element java-attribute="name" xml-path="item/name/text()" />
                <xml-element java-attribute="description" xml-path="item/description/text()" />
            </java-attributes>
        </java-type>
    </java-types>
</xml-bindings>